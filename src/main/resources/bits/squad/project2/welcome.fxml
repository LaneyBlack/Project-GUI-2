<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefWidth="800.0" prefHeight="600.0"  spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bits.squad.project2.WelcomeController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label fx:id="welcomeText" alignment="CENTER" text="Welcome to Snake 2D" textAlignment="CENTER">
      <font>
         <Font name="Calibri Light" size="20.0" />
      </font></Label>
    <Button onAction="#newGame" prefHeight="27.0" prefWidth="355.0" text="New Game">
      <font>
         <Font name="Calibri Light" size="13.0" />
      </font></Button>
    <Button onAction="#highScores" prefHeight="27.0" prefWidth="355.0" text="High Scores">
      <font>
         <Font name="Calibri Light" size="13.0" />
      </font></Button>
    <Button onAction="#exit" prefHeight="27.0" prefWidth="355.0" text="Exit">
      <font>
         <Font name="Calibri Light" size="13.0" />
      </font></Button>
</VBox>
